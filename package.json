{
  "name": "sparcl",
  "version": "0.1.6",
  "description": "A simple parser combinator library",
  "main": "dist/index.js",
  "scripts": {
    "clear": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./scripts/clearDistDirs.ps1",
    "buildRelease": "yarn clear && node_modules/.bin/tsc",
    "buildDebug": "yarn clear && node_modules/.bin/tsc --inlineSourceMap",
    "test": "node_modules/.bin/jest",
    "lint": "node_modules/.bin/eslint src/**",
    "prepublishOnly": "yarn lint && yarn build && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4erem6a/sparcl.git"
  },
  "author": "4erem6a",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "jest": "^24.9.0",
    "typescript": "^3.6.4"
  },
  "keywords": [
    "parser",
    "combinator",
    "parsing"
  ]
}
